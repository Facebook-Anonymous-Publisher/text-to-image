{
  "name": "facebook-anonymous-publisher/text-to-image",
  "description": "Convert text to image text.",
  "keywords": ["text-to-image", "text-image"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "BePsvPT",
      "email": "freedom@csie.io"
    }
  ],
  "require": {
    "php": ">=5.6",
    "ext-gd": "*",
    "ext-mbstring": "*",
    "intervention/image": "^2.3",
    "mexitek/phpcolors": "^0.4"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "^0.4",
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "FacebookAnonymousPublisher\\TextToImage\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
